<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <img id="result" src="/images/loading.gif" alt="">
    <script src="/javascripts/jquery-2.1.1.js"></script>
    <script>
    $.ajax('/image')
    .done(function(data) {
        console.log(data);
        $('#result').attr('src', data);
    }).fail(function(a,b,c) {
        console.log(b,c);
    })
    </script>
  </body>
</html>
